{
    "name": "@microsoft/live-share-media",
    "version": "2.0.0-internal.5",
    "description": "Live Share classes that synchronize media playback across clients.",
    "author": "Microsoft",
    "license": "SEE LICENSE IN LICENSE",
    "main": "./bin/cjs/index.js",
    "module": "./bin/esm/index.js",
    "types": "./bin/esm/index.d.ts",
    "type": "module",
    "scripts": {
        "clean": "npx shx rm -rf bin",
        "build": "concurrently \"tsc -p tsconfig.json\" \"tsc -p tsconfig.cjs.json && tsc-alias -p tsconfig.cjs.json && echo '{\\\"type\\\":\\\"commonjs\\\"}' > ./bin/cjs/package.json\"",
        "build:test": "tsc -p tsconfig.test.json && tsc-alias -p tsconfig.test.json && echo '{\"type\":\"commonjs\"}' > ./bin/test/package.json",
        "test": "npm run build:test && ts-mocha -p tsconfig.test.json --config .mocharc.cjs --recursive \"./bin/test/**/*.spec.js\" --timeout 10000",
        "test:debug": "npm run build:test && ts-mocha -p tsconfig.test.json --config .mocharc.cjs --recursive \"./bin/test/**/LiveMediaSession_Synchronizer.spec.js\" --inspect-brk",
        "test:coverage": "nyc --reporter=html --reporter=text --reporter=text-summary npm test",
        "test:race": "node ./../live-share/test-for-race-conditions.js"
    },
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "require": {
                "types": "./bin/cjs/index.d.ts",
                "default": "./bin/cjs/index.js"
            },
            "import": {
                "types": "./bin/esm/index.d.ts",
                "default": "./bin/esm/index.js"
            }
        }
    },
    "devDependencies": {
        "@microsoft/live-share": "2.0.0-internal.5",
        "@fluidframework/mocha-test-setup": "^2.0.0-rc.1.0.9",
        "@fluidframework/test-runtime-utils": "^2.1.0",
        "@fluidframework/test-utils": "^2.1.0",
        "@live-share-private/test-utils": "^2.0.0-internal.2",
        "@types/assert": "^1.5.6",
        "@types/mocha": "^10.0.1",
        "@types/node": "^20.2.3",
        "concurrently": "^8.2.2",
        "fluid-framework": "^2.1.0",
        "mocha": "^10.2.0",
        "nyc": "^15.1.0",
        "ts-mocha": "^10.0.0",
        "tsc-alias": "^1.8.10",
        "typescript": "~5.5.3"
    },
    "peerDependencies": {
        "@microsoft/live-share": "2.0.0-internal.5",
        "fluid-framework": "^2.1.0"
    },
    "repository": {
        "directory": "packages/live-share-media",
        "type": "git",
        "url": "https://github.com/microsoft/live-share-sdk"
    },
    "homepage": "https://aka.ms/teamsliveshare",
    "bugs": {
        "url": "https://github.com/microsoft/live-share-sdk/issues"
    }
}
